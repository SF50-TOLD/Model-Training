# NOTAM Adapter Training Environment Variables
# Copy this file to .env and fill in your values

# Anthropic API key for Claude-based labeling
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-...

# NOTAM API credentials
NOTAM_API_BASE_URL=https://notams.fly.dev
NOTAM_API_TOKEN=your-api-token

# Apple toolkit path (optional, defaults to ./adapter_training_toolkit_v26_0_0)
# TOOLKIT_PATH=./adapter_training_toolkit_v26_0_0

# App Store Connect API (for upload_adapter.sh)
# Get from: App Store Connect > Users and Access > Integrations > App Store Connect API
# ASC_ISSUER_ID=your-issuer-id
# ASC_KEY_ID=your-key-id
# ASC_PRIVATE_KEY_PATH=/path/to/AuthKey_XXXXXX.p8

# Your app's Apple ID (for upload_adapter.sh)
APP_APPLE_ID=1552835232
